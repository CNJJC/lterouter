#!/bin/bash

. ./www/tr069/common.sh

if [ "$ACTION"x = "get"x ];then
    echo 'zt1234'
    exit 0
    product_model=`uci -c /rom/etc/config get vendor.config.model 2>/dev/null`
    if [ "$product_model"x = ""x ];then
        product_model=`uci get lte_stat.status.productName 2>/dev/null`
    fi 
    echo $product_model
fi
exit 0
